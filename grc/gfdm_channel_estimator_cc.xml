<?xml version="1.0"?>
<block>
  <name>GFDM Channel Estimator</name>
  <key>gfdm_channel_estimator_cc</key>
  <category>[GFDM]</category>
  <import>import gfdm</import>
  <make>gfdm.channel_estimator_cc($timeslots, $fft_len, $active_subcarriers, $is_dc_free, $which_estimator, $preamble)</make>

  <param>
    <name>Timeslots</name>
    <key>timeslots</key>
    <type>int</type>
  </param>

  <param>
    <name>FFT length</name>
    <key>fft_len</key>
    <type>int</type>
  </param>

  <param>
    <name>Active Subcarriers</name>
    <key>active_subcarriers</key>
    <type>int</type>
  </param>

  <param>
    <name>is DC free</name>
    <key>is_dc_free</key>
    <type>bool</type>
  </param>

  <param>
    <name>Estimator Type</name>
    <key>which_estimator</key>
    <value>0</value>
    <type>int</type>
  </param>

  <param>
    <name>Preamble</name>
    <key>preamble</key>
    <type>complex_vector</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
