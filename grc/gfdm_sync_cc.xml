<block>
  <name>GFDM Sync</name>
  <key>gfdm_sync_cc</key>
  <category>[GFDM]</category>
  <import>import gfdm</import>
  <make>gfdm.sync_cc($n_subcarriers, $cp_length, $frame_len, $preamble, $gfdm_tag_key)</make>
  <param>
    <name>#subcarriers</name>
    <key>n_subcarriers</key>
    <value>32</value>
    <type>int</type>
  </param>
  <param>
    <name>CP length</name>
    <key>cp_length</key>
    <value>0</value>
    <type>int</type>
  </param>
  <param>
    <name>Frame length</name>
    <key>frame_len</key>
    <value>1024</value>
    <type>int</type>
  </param>
  <param>
    <name>Preamble</name>
    <key>preamble</key>
    <type>raw</type>
  </param>
  <param>
    <name>GFDM tag key</name>
    <key>gfdm_tag_key</key>
    <value>"gfdm_block"</value>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
